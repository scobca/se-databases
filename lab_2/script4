select
    Н_ГРУППЫ_ПЛАНОВ.ПЛАН_ИД as "ИД плана",
    COUNT(Н_ГРУППЫ_ПЛАНОВ.ГРУППА) as "Количество групп",
    Н_ДИСЦИПЛИНЫ.НАИМЕНОВАНИЕ as "Наименование дисциплины"
from
    Н_ГРУППЫ_ПЛАНОВ
inner join
    Н_ПЛАНЫ
on
    Н_ГРУППЫ_ПЛАНОВ.ПЛАН_ИД = Н_ПЛАНЫ.ИД
inner join
    Н_СТРОКИ_ПЛАНОВ
on
    Н_ПЛАНЫ.ИД = Н_СТРОКИ_ПЛАНОВ.ПЛАН_ИД
inner join
    Н_ДИСЦИПЛИНЫ
on
    Н_СТРОКИ_ПЛАНОВ.ДИС_ИД = Н_ДИСЦИПЛИНЫ.ИД
where
    Н_ДИСЦИПЛИНЫ.НАИМЕНОВАНИЕ = 'Вычислительная техника'
group by
    Н_ГРУППЫ_ПЛАНОВ.ПЛАН_ИД,
    Н_ДИСЦИПЛИНЫ.НАИМЕНОВАНИЕ
having
    COUNT(Н_ГРУППЫ_ПЛАНОВ.ГРУППА) > 2;
